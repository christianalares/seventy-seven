{
  "name": "@seventy-seven/orm",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./prisma": "./src/prisma.ts"
  },
  "scripts": {
    "lint": "biome check .",
    "check:types": "tsc --noEmit",
    "prisma:generate": "cd ../../ && npx prisma generate --schema=./packages/orm/src/prisma/schema.prisma --no-engine",
    "prisma:push": "cd ../../ && npx prisma db push --schema=./packages/orm/src/prisma/schema.prisma --skip-generate"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "@prisma/extension-accelerate": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "prisma": "^5.15.0"
  }
}
